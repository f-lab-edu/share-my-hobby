<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

    <typeAliases>
        <typeAlias type="flab.project.sharemyhobby.model.user.User" alias="User"/>
        <typeAlias type="flab.project.sharemyhobby.model.user.Email" alias="Email"/>
        <typeAlias type="flab.project.sharemyhobby.model.user.Profile" alias="Profile"/>
        <typeAlias type="flab.project.sharemyhobby.model.hobby.HobbyInfo" alias="HobbyInfo"/>
        <typeAlias type="flab.project.sharemyhobby.model.hobby.Hobby" alias="Hobby"/>
        <typeAlias type="flab.project.sharemyhobby.model.address.Address" alias="Address"/>
    </typeAliases>

    <typeHandlers>
        <typeHandler handler="org.apache.ibatis.type.LocalDateTypeHandler" />
    </typeHandlers>

</configuration>